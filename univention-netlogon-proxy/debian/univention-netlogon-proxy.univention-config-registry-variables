[samba/netlogonscript/proxy/host]
Description[de]=Name des HTTP-Proxy-Servers, der per Netlogon-Skript gesetzt werden soll
Description[en]=name of http proxy server that will be set by netlogon script
Type=str
Categories=Network

[samba/netlogonscript/proxy/port]
Description[de]=Port-Nummer des HTTP-Proxy-Servers, die per Netlogon-Skript gesetzt werden soll
Description[en]=port number of HTTP-proxy server that will be set by netlogon script
Type=str
Categories=Network

[samba/netlogonscript/proxy/whitelist]
Description[de]=Proxy-Whitelist bestehend aus Liste von IP-Adressen und Rechnernamen separiert durch Leerzeichen
Description[en]=proxy whitelist contains list of IP addresses and hostnames separated by spaces
Type=str
Categories=Network

[samba/netlogonscript/proxy/enable/firefox]
Description[de]="yes", wenn der Proxy für Firefox konfiguriert werden soll, sonst "no"
Description[en]="yes" if proxy shall be set for firefox, otherwise "no"
Type=str
Categories=Network

[samba/netlogonscript/proxy/enable/iexplorer]
Description[de]="yes", wenn der Proxy für Internet Explorer konfiguriert werden soll, sonst "no"
Description[en]="yes" if proxy shall be set for Internet Explorer, otherwise "no"
Type=str
Categories=Network

[samba/netlogonscript/proxy/firefox/binary]
Description[de]=Vollständiger Dateiname zum Firefox-Binary
Description[en]=complete path to firefox binary
Type=str
Categories=Network
