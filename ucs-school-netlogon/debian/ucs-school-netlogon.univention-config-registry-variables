[ucsschool/netlogon/.*/script]
Description[de]=Definiert Skripte (*.vbs, *.cmd, *.bat) die das netlogon Script ausführen soll (z.B., ucsschool/netlogon/<package>/script=mylogon.cmd)
Description[en]=Defines scripts (*.vbs, *.cmd, *.bat) which will be executed by the logon script (e.g., ucsschool/netlogon/<package>/script=mylogon.cmd)
Type=str
Categories=ucsschool-netlogon

[ucsschool/netlogon/.*/script/priority]
Description[de]=Definiert Ausführungsreihenfolge der netlogon Scripte (z.B., ucsschool/netlogon/<package>/script/priority=10)
Description[en]=Defines the execution order of the netlogon scripts (e.g., ucsschool/netlogon/<package>/script/priority=10)
Type=str
Categories=ucsschool-netlogon

[ucsschool/netlogon/windowStyle]
Description[de]=Setzt den Window Style für das Ausführen der definierten Logon Skripte (default: 1, möglich: 0-10)
Description[en]=Defines the windows style which will be used for executing the defined logon scripts (default: 1, possible: 0-10)
Type=str
Categories=ucsschool-netlogon

[ucsschool/netlogon/checkReturn]
Description[de]=Gibt an, ob der Rückgabewert der Logon Skript geprüft werden soll (default: true)
Description[en]=Specifies whether the return value of the logon scripts should be checked (default: true)
Type=str
Categories=ucsschool-netlogon

[ucsschool/import/set/netlogon/script/path]
Description[de]=Name des "Master" Logon Skript, dieses führt alle definierten Logon Skripte aus (default: ucs-school-logon.vbs)
Description[en]=Name of the "Master" logon script that executes all defined logon scripts (default: ucs-school-logon.vbs)
Type=str
Categories=ucsschool-netlogon

[ucsschool/netlogon/vbs/interpreter]
Description[de]=Interpreter für VBS logon Skripte (default: cscript, möglich: cscript, wscript)
Description[en]=Interpreter for VBS logon scripts (default: cscript, possible: cscript, wscript)
Type=str
Categories=ucsschool-netlogon

[ucsschool/netlogon/vbs/options]
Description[de]=Optionen für den VBS Interpreter (default: None)
Description[en]=Options for the VBS interpreter (default: None)
Type=str
Categories=ucsschool-netlogon
