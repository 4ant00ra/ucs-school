if [ "$1" = "remove" ] ; then
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/category_activearrow.gif.orig --remove /usr/share/univention-webui-style/style/category_activearrow.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/logo_univention.gif.orig --remove /usr/share/univention-webui-style/style/logo_univention.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/reiter_graurot18x24.gif.orig --remove /usr/share/univention-webui-style/style/reiter_graurot18x24.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/reiter_rot18x24.gif.orig --remove /usr/share/univention-webui-style/style/reiter_rot18x24.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/reiter_rotgrau18x24.gif.orig --remove /usr/share/univention-webui-style/style/reiter_rotgrau18x24.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/slogan_whitebg.gif.orig --remove /usr/share/univention-webui-style/style/slogan_whitebg.gif
	dpkg-divert --rename --divert /usr/share/univention-webui-style/style/stylesheet.css.orig --remove /usr/share/univention-webui-style/style/stylesheet.css
fi

#DEBHELPER#

exit 0

