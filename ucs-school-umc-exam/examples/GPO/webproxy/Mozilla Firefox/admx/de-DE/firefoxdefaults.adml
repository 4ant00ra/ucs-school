    <presentationTable>
      <presentation id="GeneralSettings">
        <textBox refId="Homepage">
          <label>Homepage</label>
        </textBox>
      </presentation>
      <presentation id="UseInternetExplorerSettings" />
      <presentation id="ProxySettings">
        <dropdownList refId="ProxySetting">Proxy Setting</dropdownList>
        <textBox refId="AutomaticProxyConfigurationURL">
          <label>Automatic Proxy Configuration URL</label>
        </textBox>
        <textBox refId="ManualSetting-HTTPProxy">
          <label>Manual Setting - HTTP Proxy</label>
        </textBox>
        <decimalTextBox refId="HTTPProxyPort" defaultValue="1">HTTP Proxy Port</decimalTextBox>
        <textBox refId="ManualSetting-SSLProxy">
          <label>Manual Setting - SSL Proxy</label>
        </textBox>
        <decimalTextBox refId="SSLProxyPort" defaultValue="1">SSL Proxy Port</decimalTextBox>
        <textBox refId="ManualSetting-FTPProxy">
          <label>Manual Setting - FTP Proxy</label>
        </textBox>
        <decimalTextBox refId="FTPProxyPort" defaultValue="1">FTP Proxy Port</decimalTextBox>
        <textBox refId="ManualSetting-GopherProxy">
          <label>Manual Setting - Gopher Proxy</label>
        </textBox>
        <decimalTextBox refId="GopherProxyPort" defaultValue="1">Gopher Proxy Port</decimalTextBox>
        <textBox refId="ManualSetting-SOCKSHost">
          <label>Manual Setting - SOCKS Host</label>
        </textBox>
        <decimalTextBox refId="SOCKSHostPort" defaultValue="1">SOCKS Host Port</decimalTextBox>
        <decimalTextBox refId="SOCKSVersion" defaultValue="4">SOCKS Version (4 or 5)</decimalTextBox>
        <textBox refId="ManualSetting-ProxyExceptions">
          <label>Manual Setting - Proxy Exceptions</label>
        </textBox>
      </presentation>
      <presentation id="EnableAutomaticImageResizing" />
      <presentation id="DisableFirefoxDefaultBrowserCheck" />
      <presentation id="Cache">
        <decimalTextBox refId="FirefoxMaximumCache" defaultValue="50000" spinStep="1000">Firefox Maximum Cache</decimalTextBox>
      </presentation>
      <presentation id="CachetoUseLocalSettings" />
      <presentation id="DisableXPIInstalls" />
      <presentation id="SetDefaultDownloadLocation">
        <dropdownList refId="DownloadTo">Download To:</dropdownList>
        <textBox refId="ManuallySetDownloadFolder">
          <label>Manually Set Download Folder (Use if Set Manually selected above)</label>
        </textBox>
      </presentation>
      <presentation id="Setmandatorybookmarksfile">
        <textBox refId="Locationofmandatorybookmarkshtmlfile">
          <label>Location of mandatory bookmarks.html file - full path to file including filename (bookmarks.html)</label>
        </textBox>
      </presentation>
      <presentation id="SetHostperm1file-UNUSED-UseWhitelistedsitesinstead">
        <textBox refId="LocationofPermissionsfile">
          <label>Location of Permissions file - full path to file including filename (hostperm.1)</label>
        </textBox>
      </presentation>
      <presentation id="WhitelistedSitefordownloadandpopups">
        <textBox refId="WhitelistforPopups">
          <label>White list for Popups</label>
        </textBox>
        <textBox refId="WhitelistforAllowedInstallsites">
          <label>White list for Allowed Install sites</label>
        </textBox>
      </presentation>
      <presentation id="EnableTypeAheadFind" />
      <presentation id="Accessibilityperuser">
        <dropdownList refId="EnableKeyboardbrowsingusingCaret">Enable Keyboard browsing using 'Caret'</dropdownList>
      </presentation>
      <presentation id="FirefoxCertificates">
        <textBox refId="SetLocationofMasterfolderthatcontainsthe3certificatefiles">
          <label>Set Location of Master folder that contains the 3 certificate files (cert8.db, key3.db, secmod.db)</label>
        </textBox>
        <dropdownList refId="Behaviour">Behaviour:</dropdownList>
      </presentation>
      <presentation id="FirefoxFileAllowedSites">
        <textBox refId="Setlistofsites">
          <label>Set list of sites, each site with a space inbetween (eg: http://www.example.com http://www.example2.com)</label>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
