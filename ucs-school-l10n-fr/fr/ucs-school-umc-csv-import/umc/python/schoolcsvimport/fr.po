#: umc/python/schoolcsvimport/__init__.py:243
msgid "Checking users from CSV file"
msgstr "Vérification des utilisateurs du fichier CSV"

#: umc/python/schoolcsvimport/__init__.py:275
msgid "Checking users from database"
msgstr "Vérification des utilisateurs de la base de données"

#: umc/python/schoolcsvimport/util.py:407
msgid "Internal Error: License check failed."
msgstr "Erreur interne : échec de la vérification de la licence."

